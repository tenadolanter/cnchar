  "bugs": {
    "url": "https://github.com/tenadolanter/cnchar/issues"
  },
  "homepage": "https://github.com/tenadolanter/cnchar#readme",
  "devDependencies": {
    "cnchar": "^3.2.5",
    "cnchar-info": "^3.2.5",
    "cnchar-radical": "^3.2.5",
    "limax": "^4.1.0",
    "puppeteer": "^21.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
