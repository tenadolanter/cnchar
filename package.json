{
  "name": "@tenado/cnchar",
  "version": "1.0.4",
  "description": "中文汉字带属性，音调、五行属性、男女属性等",
  "main": "./lib/cnchar.umd.cjs",
  "module": "./lib/cnchar.js",
  "exports": {
    ".": {
      "require": "./lib/cnchar.umd.cjs",
      "import": "./lib/cnchar.js"
    }
  },
  "files": [
    "lib",
    "types"
  ],
  "type": "module",
  "scripts": {
    "create": "node ./node/index.js",
    "update": "node ./node/update.js",
    "generare": "node ./node/test.js",
    "ts": "tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenadolanter/cnchar.git"
  },
  "author": "",
